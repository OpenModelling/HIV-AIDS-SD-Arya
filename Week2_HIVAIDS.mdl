"Death Rate (Untreated)"=
	"Population with HIV/AIDS"/"Lifespan (Untreated)"
	~	People/Year
	~		|

Order=
	2
	~	Dmnl
	~		|

Perceived Risk=
	SMOOTH N( Probability of Contact with Infected Person , Time to Adjust Perceived Risk\
		 , 0 , Order)
	~	Dmnl
	~		|

"Death Rate (Treated)"=
	Population Treated/"Lifespan (Treated)"
	~	People/Year
	~		|

"Lifespan (Treated)"=
	70
	~	Year [0,100,1]
	~		|

Effect of Probability on Contacts and Infectivity=
	Function of Probability on Contact Frequency(Perceived Risk)
	~	Dmnl
	~		|

"Population with HIV/AIDS"= INTEG (
	Testing Rate-"Death Rate (Untreated)"-Rate of Treatment,
		0)
	~	People
	~		|

Total Population=
	Population Susceptible+"Population with HIV/AIDS"+"Unidentified Population with HIV/AIDS"\
		+Population Treated
	~	People
	~		|

Total Deaths From Aids= INTEG (
	"Death Rate (Treated)"+"Death Rate (Untreated)",
		0)
	~	People
	~		|

"Lifespan (Untreated)"=
	10
	~	Year [0,15,1]
	~		|

Population Treated= INTEG (
	Rate of Treatment-"Death Rate (Treated)",
		0)
	~	People
	~		|

Fraction of Population Receiving Treatment=
	0.3
	~	Dmnl [0,0.6,0.1]
	~		|

Rate of Treatment=
	Treatment Start*((Fraction of Population Receiving Treatment*"Population with HIV/AIDS"\
		)/Time to Get Treated)
	~	People/Year
	~		|

Treatment Start=
	STEP( 1, 20 )
	~	Dmnl
	~		|

"Total Individuals Living with HIV/AIDS"=
	"Population with HIV/AIDS"+"Unidentified Population with HIV/AIDS"+Population Treated
	~	People
	~		|

Time to Get Treated=
	0.5
	~	Year [0,3,0.25]
	~		|

Probability of Contact with Infected Person=
	("Population with HIV/AIDS"+"Unidentified Population with HIV/AIDS")/Total Population
	~	Dmnl
	~		|

"Unidentified Population with HIV/AIDS"= INTEG (
	Infection Rate-Testing Rate,
		1)
	~	People
	~		|

Testing Rate=
	"Unidentified Population with HIV/AIDS"/Time to test positive
	~	People/Year
	~		|

Time to test positive=
	1
	~	Year [0,10,0.25]
	~		|

Time to Adjust Perceived Risk=
	4
	~	Year [0,50,1]
	~		|

Fear Switch=
	1
	~	Dmnl [0,1,1]
	~		|

Normal Death Rate=
	0.01
	~	1/Year [0,100,2]
	~		|

Contact Frequency=
	(Normal Contact Frequency*Effect of Probability on Contacts and Infectivity*Fear Switch\
		)+(1-Fear Switch)*Normal Contact Frequency
	~	People/People/Year
	~		|

Normal Deaths=
